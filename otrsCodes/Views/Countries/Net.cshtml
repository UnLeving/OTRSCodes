@model IEnumerable<otrsCodes.Models.Networks>
<p>
    @Html.ActionLink("Create New", "Create")
</p>

<div class="container">
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 text-center">
            <ul id="listGroup" class="list-group">
                @Html.Hidden("HexSaver", "TEST")
                @Html.Hidden("NetworkIdSaver", "TEST")
                @foreach (var item in Model)
                {
                    <li onclick="li_onclick_networks(this)"
                        class="list-group-item"
                        style="background-color:@item.Colors.Hex"
                        id="@item.Id">
                        @item.Name
                    </li>
                }

            </ul>
        </div>
    </div>
</div>